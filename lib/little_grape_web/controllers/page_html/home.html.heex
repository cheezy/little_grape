<Layouts.flash_group flash={@flash} />

<!-- Hero Section with Light Background -->
<div class="min-h-screen bg-gradient-to-b from-gray-50 via-white to-gray-100 relative overflow-hidden">
  <!-- Decorative Background Elements -->
  <div class="absolute inset-0 overflow-hidden">
    <div class="absolute -top-40 -right-40 w-80 h-80 bg-red-100/50 rounded-full blur-3xl"></div>
    <div class="absolute top-1/2 -left-20 w-60 h-60 bg-red-50/50 rounded-full blur-3xl"></div>
    <div class="absolute bottom-20 right-1/4 w-40 h-40 bg-gray-200/50 rounded-full blur-2xl"></div>
  </div>

  <!-- Navigation -->
  <nav class="relative z-10 px-6 py-4 lg:px-12 bg-white/80 backdrop-blur-sm border-b border-gray-100">
    <div class="max-w-7xl mx-auto flex justify-between items-center">
      <div class="flex items-center gap-3">
        <!-- Heart Icon for Zemra Ime -->
        <svg viewBox="0 0 100 100" class="h-12 w-12" fill="none">
          <circle cx="50" cy="50" r="48" fill="black" stroke="white" stroke-width="2"/>
          <path d="M50 75 L25 50 C15 40 15 25 30 20 C40 17 50 25 50 35 C50 25 60 17 70 20 C85 25 85 40 75 50 Z" fill="#CC0000" stroke="white" stroke-width="2"/>
        </svg>
        <span class="text-gray-900 text-2xl font-bold tracking-tight">Zemra Ime</span>
      </div>
      <div class="hidden md:flex items-center gap-8">
        <a href="#features" class="text-gray-600 hover:text-red-600 transition">{gettext("Features")}</a>
        <a href="#about" class="text-gray-600 hover:text-red-600 transition">{gettext("About")}</a>
        <a href="#" class="text-gray-600 hover:text-red-600 transition">{gettext("Success Stories")}</a>
      </div>
      <div class="flex items-center gap-4">
        <!-- Language Switcher - Simple inline approach for Safari compatibility -->
        <div class="flex items-center gap-2">
          <a
            href="/?locale=sq"
            class={"px-2 py-1 text-sm font-medium rounded transition #{if @locale == "sq", do: "bg-red-600 text-white", else: "text-gray-500 hover:text-red-600 hover:bg-red-50"}"}
          >
            SQ
          </a>
          <span class="text-gray-300">|</span>
          <a
            href="/?locale=en"
            class={"px-2 py-1 text-sm font-medium rounded transition #{if @locale == "en", do: "bg-red-600 text-white", else: "text-gray-500 hover:text-red-600 hover:bg-red-50"}"}
          >
            EN
          </a>
        </div>
<%= if @current_scope && @current_scope.user do %>
          <span class="text-gray-700">{@current_scope.user.email}</span>
          <.link href={~p"/users/profile"} class="text-gray-700 hover:text-red-600 transition font-medium">
            {gettext("Profile")}
          </.link>
          <.link href={~p"/users/log-out"} method="delete" class="bg-red-600 text-white px-6 py-2.5 rounded-full font-semibold hover:bg-red-700 transition shadow-lg">
            {gettext("Log out")}
          </.link>
        <% else %>
          <.link href={~p"/users/log-in"} class="text-gray-700 hover:text-red-600 transition font-medium">
            {gettext("Log In")}
          </.link>
          <.link href={~p"/users/register"} class="bg-red-600 text-white px-6 py-2.5 rounded-full font-semibold hover:bg-red-700 transition shadow-lg">
            {gettext("Sign Up")}
          </.link>
        <% end %>
      </div>
    </div>
  </nav>

  <!-- Hero Content -->
  <div class="relative z-10 max-w-7xl mx-auto px-6 lg:px-12 pt-16 pb-24 lg:pt-24">
    <div class="grid lg:grid-cols-2 gap-12 items-center">
      <div>
        <div class="inline-flex items-center gap-2 bg-red-50 border border-red-100 px-4 py-2 rounded-full mb-6">
          <span class="w-2 h-2 bg-red-500 rounded-full animate-pulse"></span>
          <span class="text-red-700 text-sm font-medium">{gettext("Join 50,000+ Albanians worldwide")}</span>
        </div>

        <h1 class="text-4xl md:text-5xl lg:text-6xl font-bold text-gray-900 leading-tight mb-6">
          {gettext("Find Your")}
          <span class="text-red-600"> {gettext("Albanian")} </span>
          {gettext("Soulmate")}
        </h1>

        <p class="text-gray-600 text-lg md:text-xl mb-8 max-w-xl leading-relaxed">
          {gettext("Connect with Albanians from around the world. Whether you're in Tirana, New York, London, or anywhere else ‚Äî your perfect match is waiting.")}
        </p>

        <div class="flex flex-col sm:flex-row gap-4 mb-12">
          <.link href={~p"/users/register"} class="bg-red-600 text-white px-8 py-4 rounded-full font-semibold text-lg hover:bg-red-700 transition shadow-xl text-center">
            {gettext("Start Free Today")}
          </.link>
          <a href="#features" class="bg-white text-gray-700 px-8 py-4 rounded-full font-semibold text-lg hover:bg-gray-50 transition border border-gray-200 shadow-sm text-center">
            {gettext("How It Works")}
          </a>
        </div>

        <!-- Trust Badges -->
        <div class="flex flex-wrap items-center gap-6 text-gray-500">
          <div class="flex items-center gap-2">
            <svg class="w-5 h-5 text-red-500" fill="currentColor" viewBox="0 0 20 20">
              <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"/>
            </svg>
            <span class="text-sm">{gettext("Verified Profiles")}</span>
          </div>
          <div class="flex items-center gap-2">
            <svg class="w-5 h-5 text-red-500" fill="currentColor" viewBox="0 0 20 20">
              <path fill-rule="evenodd" d="M5 9V7a5 5 0 0110 0v2a2 2 0 012 2v5a2 2 0 01-2 2H5a2 2 0 01-2-2v-5a2 2 0 012-2zm8-2v2H7V7a3 3 0 016 0z" clip-rule="evenodd"/>
            </svg>
            <span class="text-sm">{gettext("100% Private")}</span>
          </div>
          <div class="flex items-center gap-2">
            <svg class="w-5 h-5 text-red-500" fill="currentColor" viewBox="0 0 20 20">
              <path d="M9.049 2.927c.3-.921 1.603-.921 1.902 0l1.07 3.292a1 1 0 00.95.69h3.462c.969 0 1.371 1.24.588 1.81l-2.8 2.034a1 1 0 00-.364 1.118l1.07 3.292c.3.921-.755 1.688-1.54 1.118l-2.8-2.034a1 1 0 00-1.175 0l-2.8 2.034c-.784.57-1.838-.197-1.539-1.118l1.07-3.292a1 1 0 00-.364-1.118L2.98 8.72c-.783-.57-.38-1.81.588-1.81h3.461a1 1 0 00.951-.69l1.07-3.292z"/>
            </svg>
            <span class="text-sm">{gettext("4.8 Star Rating")}</span>
          </div>
        </div>
      </div>

      <!-- Hero Image/Card -->
      <div class="hidden lg:block">
        <div class="relative">
          <!-- Decorative cards behind -->
          <div class="absolute -left-4 top-8 w-72 h-96 bg-red-100/50 rounded-3xl transform -rotate-6"></div>
          <div class="absolute -right-4 top-4 w-72 h-96 bg-gray-200/50 rounded-3xl transform rotate-6"></div>

          <!-- Main card -->
          <div class="relative bg-white rounded-3xl p-6 shadow-2xl transform hover:scale-105 transition-transform duration-300 border border-gray-100">
            <div class="aspect-[3/4] bg-gradient-to-br from-gray-50 to-gray-100 rounded-2xl mb-4 flex items-center justify-center overflow-hidden">
              <div class="text-center p-8">
                <div class="w-32 h-32 mx-auto bg-gradient-to-br from-red-500 to-red-700 rounded-full flex items-center justify-center mb-4 shadow-lg">
                  <svg class="w-16 h-16 text-white" fill="currentColor" viewBox="0 0 20 20">
                    <path fill-rule="evenodd" d="M3.172 5.172a4 4 0 015.656 0L10 6.343l1.172-1.171a4 4 0 115.656 5.656L10 17.657l-6.828-6.829a4 4 0 010-5.656z" clip-rule="evenodd"/>
                  </svg>
                </div>
                <p class="text-gray-600 font-medium">{gettext("Your match awaits")}</p>
              </div>
            </div>
            <div class="text-center">
              <h3 class="text-gray-900 font-bold text-xl">{gettext("Find Your Perfect Match")}</h3>
              <p class="text-gray-500 mt-1">{gettext("Based on culture & compatibility")}</p>
              <div class="flex justify-center gap-1 mt-3">
                <span class="w-8 h-1 bg-red-600 rounded-full"></span>
                <span class="w-8 h-1 bg-black rounded-full"></span>
                <span class="w-8 h-1 bg-red-600 rounded-full"></span>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<!-- Features Section -->
<section id="features" class="py-24 bg-white">
  <div class="max-w-7xl mx-auto px-6 lg:px-12">
    <div class="text-center mb-16">
      <h2 class="text-3xl md:text-4xl font-bold text-gray-900 mb-4">
        {gettext("Why Choose Zemra Ime?")}
      </h2>
      <p class="text-gray-600 text-lg max-w-2xl mx-auto">
        {gettext("Built by Albanians, for Albanians. We understand our culture, traditions, and what matters in finding a life partner.")}
      </p>
    </div>

    <div class="grid md:grid-cols-3 gap-8">
      <!-- Feature 1 -->
      <div class="bg-gray-50 rounded-2xl p-8 hover:shadow-lg transition-shadow border border-gray-100">
        <div class="w-14 h-14 bg-red-600 rounded-xl flex items-center justify-center mb-6">
          <svg class="w-7 h-7 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3.055 11H5a2 2 0 012 2v1a2 2 0 002 2 2 2 0 012 2v2.945M8 3.935V5.5A2.5 2.5 0 0010.5 8h.5a2 2 0 012 2 2 2 0 104 0 2 2 0 012-2h1.064M15 20.488V18a2 2 0 012-2h3.064M21 12a9 9 0 11-18 0 9 9 0 0118 0z"/>
          </svg>
        </div>
        <h3 class="text-xl font-bold text-gray-900 mb-3">{gettext("Global Albanian Community")}</h3>
        <p class="text-gray-600 leading-relaxed">
          {gettext("Connect with Albanians from Kosovo, Albania, North Macedonia, Montenegro, and diaspora communities worldwide.")}
        </p>
      </div>

      <!-- Feature 2 -->
      <div class="bg-gray-50 rounded-2xl p-8 hover:shadow-lg transition-shadow border border-gray-100">
        <div class="w-14 h-14 bg-black rounded-xl flex items-center justify-center mb-6">
          <svg class="w-7 h-7 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m5.618-4.016A11.955 11.955 0 0112 2.944a11.955 11.955 0 01-8.618 3.04A12.02 12.02 0 003 9c0 5.591 3.824 10.29 9 11.622 5.176-1.332 9-6.03 9-11.622 0-1.042-.133-2.052-.382-3.016z"/>
          </svg>
        </div>
        <h3 class="text-xl font-bold text-gray-900 mb-3">{gettext("Verified & Safe")}</h3>
        <p class="text-gray-600 leading-relaxed">
          {gettext("Every profile is verified. Your privacy and security are our top priority with advanced safety features.")}
        </p>
      </div>

      <!-- Feature 3 -->
      <div class="bg-gray-50 rounded-2xl p-8 hover:shadow-lg transition-shadow border border-gray-100">
        <div class="w-14 h-14 bg-red-600 rounded-xl flex items-center justify-center mb-6">
          <svg class="w-7 h-7 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4.318 6.318a4.5 4.5 0 000 6.364L12 20.364l7.682-7.682a4.5 4.5 0 00-6.364-6.364L12 7.636l-1.318-1.318a4.5 4.5 0 00-6.364 0z"/>
          </svg>
        </div>
        <h3 class="text-xl font-bold text-gray-900 mb-3">{gettext("Smart Matching")}</h3>
        <p class="text-gray-600 leading-relaxed">
          {gettext("Our algorithm considers cultural values, family traditions, and personal preferences for meaningful connections.")}
        </p>
      </div>
    </div>
  </div>
</section>

<!-- Stats Section -->
<section class="py-16 bg-gray-900">
  <div class="max-w-7xl mx-auto px-6 lg:px-12">
    <div class="grid grid-cols-2 md:grid-cols-4 gap-8 text-center">
      <div>
        <div class="text-4xl md:text-5xl font-bold text-white mb-2">50K+</div>
        <div class="text-gray-400">{gettext("Active Members")}</div>
      </div>
      <div>
        <div class="text-4xl md:text-5xl font-bold text-red-500 mb-2">12K+</div>
        <div class="text-gray-400">{gettext("Successful Matches")}</div>
      </div>
      <div>
        <div class="text-4xl md:text-5xl font-bold text-white mb-2">45+</div>
        <div class="text-gray-400">{gettext("Countries")}</div>
      </div>
      <div>
        <div class="text-4xl md:text-5xl font-bold text-red-500 mb-2">4.8</div>
        <div class="text-gray-400">{gettext("App Store Rating")}</div>
      </div>
    </div>
  </div>
</section>

